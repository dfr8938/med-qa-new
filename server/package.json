      "routes/**/*.js"
    ]
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "sequelize"
  ],
  "author": "SourceCraft Developer",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "clone": "^2.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.0",
    "node-cache": "^5.1.2",
    "pg": "^8.18.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2"
  }
