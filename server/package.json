{
  "name": "med-qa-backend",
  "version": "1.0.0",
  "description": "Backend for Medical Q&A Portal",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "setup-db": "node scripts/create_db.js",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
